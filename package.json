{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate for my team",
  "main": "index.js",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "analyze": "pnpm build --env withAnalyze=true",
    "dev": "webpack serve --config internals/webpack/webpack.dev.js",
    "start": "serve build",
    "build": "cross-env TARGET_ENV=production webpack --config internals/webpack/webpack.prod.js",
    "build:staging": "cross-env TARGET_ENV=staging webpack --config internals/webpack/webpack.prod.js",
    "lint": "eslint . --ext .js,.jsx,.mjs --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.mjs --ignore-path .gitignore --fix",
    "prettify": "prettier --write",
    "pre-commit": "lint-staged",
    "test": "node internals/testing/test-runner.js",
    "test:coverage": "rimraf ./coverage && pnpm lint && jest --coverage",
    "generate": "node internals/generators/index.mjs",
    "g": "node internals/generators/index.mjs",
    "i18n:extract": "node internals/sheets/extract.mjs",
    "i18n:compile": "node internals/sheets/compile.mjs",
    "i18n:push": "node internals/sheets/push.mjs",
    "i18n:pull": "node internals/sheets/pull.mjs",
    "doc": "doctoc .",
    "prepare": "cd .. && husky install vonvon/.husky"
  },
  "keywords": [],
  "author": "linh2t",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "cd vonvon/ && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pnpm lint:fix",
      "pnpm prettify"
    ],
    "*.{json,css,md}": [
      "pnpm prettify"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not IE 11",
      "not op_mini all",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "antd": "^5.0.0",
    "core-js": "^3.26.1",
    "dayjs": "^1.11.6",
    "framer-motion": "^7.10.2",
    "jotai": "^1.10.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "react-router-dom": "^6.4.3",
    "styled-components": "^5.3.6",
    "swr": "^1.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@formatjs/cli": "^5.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@squoosh/lib": "^0.4.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-formatjs": "^10.3.31",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.21.4",
    "chalk": "^5.1.2",
    "commander": "^9.4.1",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "doctoc": "^2.2.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^109.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "inquirer": "^9.1.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "loader-utils": "^3.2.1",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "replace-in-file": "^6.3.5",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "serve": "^14.1.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "sideEffects": false,
  "packageManager": "^pnpm@7.17.0",
  "resolutions": {
    "styled-components": "^5"
  }
}
